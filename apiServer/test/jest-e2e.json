{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": ["ts-jest", {
      "useESM": false,
      "tsconfig": {
        "module": "commonjs"
      }
    }]
  },
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1",
    "^uuid$": "<rootDir>/../node_modules/uuid/dist-node/index.js"
  }
}
