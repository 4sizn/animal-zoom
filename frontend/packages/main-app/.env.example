# Environment Configuration Example
# Copy this file to .env.development for local development

# API Configuration
# Backend API server URL
VITE_API_URL=http://localhost:3000
# Note: API endpoints are at root level (e.g., /rooms, /health)
# Leave empty for no prefix
VITE_API_PREFIX=

# WebSocket Configuration
# WebSocket server URL for real-time communication
VITE_WS_URL=http://localhost:3001

# Debug Mode
# Set to 'true' to enable API request/response logging
VITE_DEBUG=true

# Environment
NODE_ENV=development
