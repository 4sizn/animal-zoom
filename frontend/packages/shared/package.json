  "dependencies": {
    "socket.io-client": "^4.8.0",
    "axios": "^1.7.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "~5.9.3",
